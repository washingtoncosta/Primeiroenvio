#include <stdio.h>

int main(){//Definição das variaveis a ser utilizada no programa. 
    int numero, coluna, linha;
    double matriz[12][12], soma = 0, media = 0;
    char operacao;

    scanf("%d", &numero);//Declarando as veariaveis em que o programa irá receber. 
    scanf(" %c", &operacao);
    if(numero < 0 || numero > 11){//Solicitando um numero de 0 a 11 para o usuário.
    printf("Numero invalido\n");
    }
    else{//Nessa parte é feita a coleta e a soma/media dos numeros informado pelo usuario.  
    for(coluna = 0; coluna <= 11; coluna++){
    for(linha = 0; linha <= 11; linha++){
        scanf("%lf", &matriz[linha][coluna]);
            if(linha == numero){
                if(operacao == 'S'){
                soma = soma + matriz[linha][coluna];
                }
                else if(operacao == 'M'){
                media = media + matriz[linha][coluna];
                }
            }
        }
    }
    media = media / 12;//Realiza a media da linha que o usuário escolheu.
    if(operacao == 'S'){//Apresentando ao usuário a SOMA da linha escolhida, caso ele tenha escolhido a opção SOMA no inicio do programa.
    printf("%.1lf\n", soma);
    }
    else if(operacao == 'M'){//Apresentando ao usuário a MEDIA da linha escolhida, caso ele tenha escolhido a opção MEDIA no inicio do programa.
    printf("%.1lf\n", media);
    }
    }
}
