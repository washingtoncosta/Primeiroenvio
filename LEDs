#include <stdio.h>
#include <stdlib.h>
#include <string.h>
int main(){
	int numero, soma = 0, a, b; // Definição das variaveis a ser utilizada no programa. 
	char vetor[10000];
	scanf("%d", &numero);//Declarando a veariavel em que o programa irá receber
	for(a = 0; a < numero; a++){// De acordo com o numero que o usuario digitar vai ser a quantidade de vezes que ele podera informar para saber quantos leds vai precisar para formar o numero. 
	    scanf("%s", vetor);
		for(b = 0; b < strlen(vetor) ; b++){
		switch(vetor[b]){// De acordo com o numero que foi digitado vai sendo somado quantos Leds precisa para formar o numero informado. 
			case '0': soma = soma + 6; 
			break;
			case '1': soma = soma + 2;
			break;
			case '2': soma = soma + 5;
			break;
			case '3': soma = soma + 5;
			break;
			case '4': soma = soma + 4;
			break;
			case '5': soma = soma + 5;
			break;
			case '6': soma = soma + 6;
			break;
			case '7': soma = soma + 3;
			break;
			case '8': soma = soma + 7;
			break;
			case '9': soma = soma + 6;
			break;
			}	
			}
			printf("%d leds\n", soma);// Apresenta na tela a quantidade de Leds que precisa para formar o numero escolhido pelo usuario. 
			soma = 0;
	}
	system("pause");
}
	
